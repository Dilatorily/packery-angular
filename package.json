{
  "name": "packery-angular",
  "version": "0.0.2",
  "description": "AngularJS module for Packery",
  "main": "dist/packery-angular.js",
  "scripts": {
    "test": "gulp test",
    "preversion": "git pull && npm test",
    "version": "gulp dist && git add . --all && git commit -m '[CHORE] Generate dist files for the release'",
    "postversion": "git push && git push --tags",
    "prepublish:major": "gulp version:major && npm run version",
    "publish:major": "npm run publish",
    "prepublish:minor": "gulp version:minor && npm run version",
    "publish:minor": "npm run publish",
    "prepublish:patch": "gulp version:patch && npm run version",
    "publish:patch": "npm run publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dilatorily/packery-angular.git"
  },
  "keywords": [
    "AngularJS",
    "Packery",
    "Draggabilly"
  ],
  "author": "Dilatorily",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Dilatorily/packery-angular/issues"
  },
  "homepage": "https://github.com/Dilatorily/packery-angular#readme",
  "dependencies": {
    "angular": "^1.4.8",
    "draggabilly": "^2.0.1",
    "packery": "^1.4.3"
  },
  "devDependencies": {
    "angular-mocks": "^1.4.8",
    "gulp": "^3.9.0",
    "gulp-bump": "^1.0.0",
    "gulp-coveralls": "^0.1.4",
    "gulp-filter": "^3.0.1",
    "gulp-git": "^1.6.1",
    "gulp-jscs": "^3.0.2",
    "gulp-jscs-stylish": "^1.3.0",
    "gulp-jshint": "^2.0.0",
    "gulp-load-plugins": "^1.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-tag-version": "^1.3.0",
    "gulp-uglify": "^1.5.1",
    "jasmine-core": "^2.4.1",
    "jshint": "^2.8.0",
    "jshint-stylish": "^2.1.0",
    "karma": "^0.13.19",
    "karma-coverage": "^0.5.3",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^0.2.3",
    "phantomjs": "^1.9.19",
    "run-sequence": "^1.1.5"
  }
}
